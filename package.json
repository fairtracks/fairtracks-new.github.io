{
  "name": "fairtracks.github.io",
  "version": "1.0.0",
  "private": true,
  "browserslist": "> 0.12%, last 2 versions, Firefox ESR, not dead",
  "scripts": {
    "reinstall": "rm -rf node_modules .nuxt dist package-lock.json; npm install",
    "update": "npx npm-check-updates -u; npm run reinstall",
    "dev": "nuxt --port 5050",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "static": "npm run generate; npm run start",
    "rebuildstatic": "rm -rf dist .nuxt node_modules/.cache/nuxt; npm run static",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "test": "jest"
  },
  "dependencies": {
    "nuxt": "^2.15.8"
  },
